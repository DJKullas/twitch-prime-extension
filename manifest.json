{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
      ],
      "content_scripts": [
        {
          "matches": ["*://*.twitch.tv/*"],
          "run_at": "document_idle",
          "js": ["content.js"]
        }
      ]
  }